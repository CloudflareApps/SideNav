{
  "resources": {
    "head": [
      {
        "type": "style",
        "src": "./source/app.css"
      },
      {
        "type": "script",
        "src": "./source/app.js"
      }
    ]
  },
  "preview": {
    "handlers": [
      {
        "options": ["*"],
        "execute": "INSTALL_SCOPE.setOptions(INSTALL_OPTIONS)"
      }
    ]
  },
  "options": {
    "properties": {
      "position": {
        "order": 1,
        "title": "Placement",
        "type": "string",
        "enum": [
          "left",
          "right"
        ],
        "enumNames": {
          "left": "Left side of the page",
          "right": "Right side of the page"
        },
        "format": "radios",
        "default": "left"
      },
      "labelColor": {
        "order": 2,
        "title": "Label Text Color",
        "type": "string",
        "format": "color",
        "default": "#888888"
      },
      "linkColor": {
        "order": 3,
        "title": "Link Text Color",
        "type": "string",
        "format": "color",
        "default": "#000000"
      },
      "backgroundColor": {
        "order": 4,
        "title": "Nav Background Color",
        "type": "string",
        "format": "color",
        "default": "#ffffff"
      },
      "buttonColor": {
        "order": 5,
        "title": "Nav Menu Icon Color",
        "type": "string",
        "format": "color",
        "default": "#000000"
      },
      "showCover": {
        "order": 6,
        "title": "Cover the page when the side nav is open.",
        "type": "boolean",
        "default": true
      },
      "coverColor": {
        "order": 7,
        "showIf": {
          "showCover": true
        },
        "title": "Cover color",
        "type": "string",
        "format": "color",
        "default": "#000000"
      },
      "items": {
        "order": 8,
        "title": "Nav Items",
        "description": "Add as many items to your Nav as youâ€™d like. The two types of items you can add are <strong>Text Labels</strong> and <strong>Links</strong>. Drag and drop to re-order your nav items.",
        "type": "array",
        "items": {
          "type": "object",
          "title": "Nav Item",
          "properties": {
            "itemType": {
              "order": 1,
              "title": "Type of nav item",
              "type": "string",
              "enum": [
                "label",
                "link"
              ],
              "enumNames": {
                "label": "Text Label",
                "link": "Link"
              },
              "default": "link"
            },


            "labelText": {
              "order": 2,
              "showIf": {
                "items[].itemType": "label"
              },
              "title": "Label Text",
              "type": "string"
            },
            "linkText": {
              "order": 3,
              "showIf": {
                "items[].itemType": "link"
              },
              "title": "Link Text",
              "type": "string"
            },


            "href": {
              "order": 4,
              "showIf": {
                "items[].itemType": "link"
